"import { Body, Controller, Delete, Get, Inject, Param, Post, Put, Query } from "@nestjs/common";
import { ApiTags } from "@nestjs/swagger";
import { CreatePetEndpoint, DeletePetEndpoint, GetPetByIdEndpoint, ListPetsEndpoint, UpdatePetEndpoint } from "../decorators/pets.decorator";
import { NewPet } from "../dtos/NewPet.dto";
import { Pet } from "../dtos/Pet.dto";

export interface IPetsService {
    listPets(...args: any[]): Promise<Pet[]>;
    createPet(...args: any[]): Promise<Pet>;
    getPetById(...args: any[]): Promise<Pet>;
    updatePet(...args: any[]): Promise<Pet>;
    deletePet(...args: any[]): Promise<any>;
}

@ApiTags('pets')
@Controller('api/pets')
export class PetsController {
    constructor(@Inject('IPetsService') private readonly service: IPetsService) {
    }

    @Get('')
    @ListPetsEndpoint()
    async listPets(@Query('limit') limit: number, @Query('status') status: string): Promise<Pet[]> {
        return this.service.listPets(limit, status);
    }

    @Post('')
    @CreatePetEndpoint()
    async createPet(@Body() body: NewPet): Promise<Pet> {
        return this.service.createPet(body);
    }

    @Get(':petId')
    @GetPetByIdEndpoint()
    async getPetById(@Param('petId') petId: number): Promise<Pet> {
        return this.service.getPetById(petId);
    }

    @Put(':petId')
    @UpdatePetEndpoint()
    async updatePet(@Param('petId') petId: number, @Body() body: NewPet): Promise<Pet> {
        return this.service.updatePet(petId, body);
    }

    @Delete(':petId')
    @DeletePetEndpoint()
    async deletePet(@Param('petId') petId: number): Promise<any> {
        return this.service.deletePet(petId);
    }
}"
`;

